<?xml version="1.0" encoding="utf-8" ?>

<odoo>
    <record id="rezervation_city_action" model='ir.actions.act_window'>
        <field name="name">City</field>
        <field name="res_model">rezervation.city</field>
        <field name="view_mode">kanban,tree,form,activity</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">Create the city</p>
        </field>
    </record>


    <record id="rezervation_city_tree" model="ir.ui.view">
        <field name="name">City.List.tree</field>
        <field name="model">rezervation.city</field>
        <field name="arch" type="xml">
            <tree string="City_tree">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="rezervation_city_view_form" model="ir.ui.view">
        <field name="name">City</field>
        <field name="model">rezervation.city</field>
        <field name="arch" type="xml">
            <form>
                <group string="City">
                    <field name="name"/>
                </group>

            </form>
        </field>
    </record>




</odoo>